{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"nodemon dist/app.js\"",
    "prod": "npm run tsc && pm2 start dist/app.js --name api --no-daemon",
    "build": "npm run tsc:es6",
    "postinstall": "npm run build",
    "tslint": "tslint ./*.ts ./lib/*.ts",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tsc:es6": "tsc --project tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "new-migration": "tsc && \"./node_modules/.bin/typeorm\" migrations:generate --name",
    "migrations": "tsc && \"./node_modules/.bin/typeorm\" migrations:run",
    "prepare:db": "tsc && node dist/prepare_db.js"
  },
